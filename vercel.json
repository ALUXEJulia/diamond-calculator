{
  "version": 2,
  "public": true,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "App.js", "use": "@vercel/static" },
    { "src": "index.js", "use": "@vercel/static" },
    { "src": "App.css", "use": "@vercel/static" },
    { "src": "diamond_data.json", "use": "@vercel/static" },
    { "src": "logo.png", "use": "@vercel/static" }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "headers": {
        "Content-Type": "text/html; charset=utf-8"
      }
    }
  ]
}