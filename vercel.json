{
  "version": 2,
  "public": true,
  "builds": [
    { "src": "igi-price.html", "use": "@vercel/static" },
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "logo.png", "use": "@vercel/static" },
    { "src": "diamond_data.json", "use": "@vercel/static" }
