{
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/logo.png",
      "dest": "/public/logo.png"
    },
    {
      "src": "/igi-price.html",
      "dest": "/public/igi-price.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/igi-price.html"
    }
  ]
}
